{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CorsOptionsDefault": {
    "PolicyName": "default",
    "Origins": [ "http://localhost:3000", "https://example.com" ],
    "Methods": [ "GET", "POST", "PUT", "DELETE" ],
